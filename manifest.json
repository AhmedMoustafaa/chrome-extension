{
  "manifest_version": 3,
  "name": "Select Manipulator",
  "version": "1.0",
  "description": "A Chrome extension that manipulates select elements on a page.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
